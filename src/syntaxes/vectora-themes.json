{
  "type": "dark-modern",
  "colors": {
    "editor.foreground": "#D4D4D4",
    "editor.background": "#1E1E1E"
  },
  "tokenColors": [
    {
      "scope": "entity.name.tag.cssc",
      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": "entity.other.attribute-name.class.cssc",
      "settings": {
        "foreground": "#D7BA7D" 
      }
    },
    {
      "scope": "entity.other.attribute-name.id.cssc",
      "settings": {
        "foreground": "#9CDCFE" 
      }
    },
    {
      "scope": "entity.orphan.element-name.async.cssc",
      "settings": {
        "foreground": "#f09cf3"
      }
    },
    {
      "scope": "keyword.control.event.cssc",
      "settings": {
        "foreground": "#C586C0"
      }
    },
    {
      "scope": "support.constant.property-name.cssc",
      "settings": {
        "foreground": "#4FC1FF"
      }
    },
    {
      "scope": "support.function.animation.cssc",
      "settings": {
        "foreground": "#DCDCAA"
      }
    },
    {
      "scope": "support.operator.vec",
      "settings": {
        "foreground": "#b0b000"
      }
    },
    {
      "scope": "support.property-value.vec",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": [
        "comment.line.double-slash.cssc",
        "comment.block.cssc"
      ],
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "scope": [
        "punctuation.definition.block.cssc",
        "punctuation.section.block.begin.cssc",
        "punctuation.section.block.end.cssc"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    }
  ]
}